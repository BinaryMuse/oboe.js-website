 
<section id="demoArea">

   <svg width="500" height="200">
     
     <rect id="background" x="0" y="0" width="500" height="200"></rect>

       <g id="wires">
           <template id="wire">
               <line class="wire" />
           </template>
       </g>

       <g id="places">
         <template id="server">
            <rect class="server" x="-22.5" y="-40" width="45" height="80"></rect>
         </template>
         <template id="client">
            <rect class="client" x="-22.5" y="-40" width="45" height="80"></rect>
         </template>
      </g>
              
      <g id="packets">
         <template id="packet">
            <circle transform="translate(0,0)" class="packet" r="{{packetRadius}}"></circle>
         </template>
         <template id="firstPacket">
            <g transform="translate(0,0)" class="packet"> 
               <circle r="{{packetRadius}}"></circle>
               <text>{</text>
            </g>
         </template>
          <template id="lastPacket">
              <g transform="translate(0,0)" class="packet">
                  <circle r="{{packetRadius}}"></circle>
                  <text>}</text>
              </g>
          </template>
          <template id="getRequest">
              <g transform="translate(0,0)" class="packet">
                  <circle r="{{packetRadius}}"></circle>
                  <text>GET</text>
              </g>
          </template>          
      </g>

   </svg>         

   <ul>
      <!--<li><a href="">&lt;</a></li>-->
      <li><a href="/demo/1">1</a></li>
      <li><a href="/demo/2">2</a></li>
      <li><a href="/demo/3">3</a></li>
      <li><a href="/demo/4">4</a></li>
      <!--<li><a href="">&gt;</a></li>      -->
   </ul>
</section>
