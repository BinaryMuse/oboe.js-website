<svg id="demoTemplateHolder">
    <template id="demo">
        <svg width="500" height="200">
            <g class="scaling">
               <g class="wires">
               </g>
   
               <g class="messages">
               </g>
   
               <g class="packets">
               </g>

               <g class="barriers">
               </g>
               
               <g class="places">
               </g>
   
