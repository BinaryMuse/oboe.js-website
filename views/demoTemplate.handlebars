                 d="m -105,-124 0,169 25,0 0,-84 c 0,-35.89851 47.010102,-65 105,-65 57.989899,0 105,29.10149 105,65 l 0,84 25,0 0,-169 z"
           />
           <path class="downstream"
                 d="m 25,-104 c -57.989898,0 -105,29.10149 -105,65 l 0,84 -60,60 330,0 -60,-60 0,-84 c 0,-35.89851 -47.010101,-65 -105,-65 z"
           />
          
           <path class="upstream"
                 d="m -105,45 60,60 -435,0 0,-320 640,0 0,50 -265,41 z"
           />
        </g>
    </template>

</svg>

