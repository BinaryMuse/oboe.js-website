<!DOCTYPE html>
<html>
    <head>
    
       <meta name="viewport" content="width=device-width, initial-scale=1.0"/>

       {{#each stylesheets}}
          <link rel="stylesheet" href="/css/{{this}}">
       {{/each}}
    
       <title>Oboe.js</title>
    
       <meta name="Description" content="Progressive loading of json"/>
           
    </head>
    <body>
        <div id="header">
               {{#if experimental}}
                    <div class="button2 wee"></div>
                    <div class="button2 wee"></div>
                    <div class="button2 wee"></div>
                    <div class="button2"></div>
                    <div class="button2"></div>
                    <div class="button2"></div>
                    <div class="button2"></div>
               {{/if}}
        </div>
        <nav id="siteNav">
            <svg width="24" height="24" class="menuButton" >
                <rect class="background" width="24" height="24" />
                <rect class="rung" width="16" height="4" x="4" y="3"  />
                <rect class="rung" width="16"  height="4" x="4" y="10"  />
                <rect class="rung" width="16" height="4" x="4" y="17" />
            </svg>
           <ul>
              {{#home}}
                  <li class="reducedLogo">Oboe.js</li>
              {{/home}}
              {{^home}}
                  <li class="reducedLogo"><a href="/">Oboe.js</a></li>
              {{/home}}
                              
              {{#pages}}
                  <li>
                     {{#if current}}
                         <span class="siteLink">{{title}}</span>
                     {{else}}
                         <a class="siteLink" href="/{{path}}/">{{title}}</a>
                     {{/if}}
                  </li>

              {{/pages}}
           </ul>
        </nav>        
        <div id="pagearea">
           <header class="row">
              {{#home}}
                  <h1>Oboe.js</h1>
              {{/home}}
              {{^home}}
                  <h1><a href="/">Oboe.js</a></h1>
              {{/home}}
        
              {{#if home}}
                 <p class="strapline">{{heading.text}}</p>
              {{/if}}
           </header>
        
           <div class="row">
        
              <nav class="col col1">
                 {{#if home}}
                     <a href="{{releasedJs}}/dist/oboe-browser.js"
                        id="download" title="Get the code">
                        Download <span id="version">{{latestTag}}</span>
                     </a>
                 {{else}}                 
                    <div class="internalNav">
                       <h2>{{heading.text}}</h2>
                       {{#if sections}}         
                          <ul class="sections">
                             {{#sections}}
                                <li><a href="#{{id}}">{{text}}</a></li>
                             {{/sections}}
                          </ul>
                       {{/if}}
                    </div>
                 {{/if}}

                 {{#if experimental}}
                    <div class="siteNav">
                     <ul class="sections site">
                        <li><a href="api">API</a></li>
                        <li><a href="examples">Examples</a></li>
                        <li><a href="installing">Installing</a></li>
                        <li><a href="useCases">Use cases</a></li>
                        <li><a href="why">Why Oboe.js?</a></li>
                     </ul>
                    </div>
                 {{/if}}
        
                 {{#home}}               
                     <ul class="links">
                        <li><a href="https://github.com/jimhigson/oboe.js">Github</a></li>
                        <li><a href="https://twitter.com/OboeJs">Twitter</a>
                        </li>
                        <li><a href="https://npmjs.org/package/oboe/">npm</a></li>
                        <li>
                           <a href="https://twitter.com/OboeJs"
                              class="twitter-follow-button"
                              data-show-count="false"
                              data-lang="en"
                              data-size="large">
                              Follow @OboeJs
                           </a>
                        </li>
                     </ul>
                 {{/home}}
              </nav>
              <div class="col col2">
        
                 <main class="marked">
                    {{{content}}}
                 </main>
        
              </div>
        
           </div>
        </div>
        <footer>
           <div id="footer-contents">
              <a href="//samwakeling.com" id="sw" title="Web design by Sam Wakeling">Web
                 design by Sam Wakeling</a>
        
              <p><a href="mailto:jim.higson@gmail.com">Contact the author</a></p>
           </div>
        </footer>
        
        {{#if twitter}}
           <script type="text/javascript">!function (d, s, id) {
              var js, fjs = d.getElementsByTagName(s)[0];
              if (!d.getElementById(id)) {
                 js = d.createElement(s);
                 js.id = id;
                 js.src = "//platform.twitter.com/widgets.js";
                 fjs.parentNode.insertBefore(js, fjs);
              }
           }(document, "script", "twitter-wjs");</script>
        {{/if}}

        <script src="/jquery/jquery.js"></script>
        {{#each scripts}}
            <script type="text/javascript" src="{{this}}"></script>
        {{/each}}    
        {{#if initScript}}
            <script type="text/javascript">{{initScript}}</script>           
        {{/if}}
    
        {{{templates}}}
    </body>
</html>
